{
    "name": "skychin/jigsaw-webp",
    "type": "library",
    "description": "Convert JPEG and PNG images to WebP for static website generated by Jigsaw",
    "keywords": [
        "skychin",
        "jigsaw-webp"
    ],
    "homepage": "https://github.com/co0lsky/jigsaw-webp",
    "license": "MIT",
    "authors": [
        {
            "name": "Sky Chin",
            "email": "sky4just@gmail.com",
            "homepage": "http://www.skychin.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/filesystem": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SkyChin\\Jigsaw\\WebP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SkyChin\\Jigsaw\\WebP\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
